    <div class="pk-part2">
      <header>
        <div class="profile">
          <img src="/Images/Uploads/<%= user.profile_pic %>" alt="Profile Picture" />
          <span><%= user.name %></span>
        </div>
      </header>

      <div id="service-main">
        <div id="servewrap">
         
  
          <table class="services-table">
            <% for (let i = 0; i < manager.services.parking.parking_slots; i++) { %>
                <% if (i % 4 === 0) { %>
                    <tr>
                <% } %>
                <td onclick="toggleSelected(this) "><%= i + 1 %></td>
                <% if ((i + 1) % 4 === 0 || (i + 1) === manager.services.parking.parking_slots) { %>
                    </tr>
                <% } %>
            <% } %>
        </table>
        
        </div>
  
